{
    "name": "Visual Studio Code(Chrome Extension)",
    "description": "Unofficial extension for Visual Studio Code",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "serviceWorker.js"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "/images/icon_16.png",
            "32": "/images/icon_32.png",
            "48": "/images/icon_48.png",
            "128": "/images/icon_128.png"
        }
    },
    "icons": {
        "16": "/images/icon_16.png",
        "32": "/images/icon_32.png",
        "48": "/images/icon_48.png",
        "128": "/images/icon_128.png"
    }
}